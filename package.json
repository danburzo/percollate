{
	"name": "percollate",
	"version": "0.2.2",
	"description": "A command-line tool to grab web pages as PDF",
	"main": "index.js",
	"repository": "git@github.com:danburzo/percollate.git",
	"author": "Dan Burzo <danburzo@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"commander": "^2.18.0",
		"css": "^2.2.4",
		"got": "^9.2.2",
		"jsdom": "^12.1.0",
		"nunjucks": "^3.1.3",
		"puppeteer": "^1.8.0",
		"tmp": "^0.0.33"
	},
	"bin": {
		"percollate": "./index.js"
	},
	"engines": {
		"node": ">= 8.0.0"
	},
	"devDependencies": {
		"husky": "^1.0.1",
		"prettier": "^1.14.3",
		"pretty-quick": "^1.7.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "none",
		"useTabs": true,
		"tabWidth": 4,
		"jsxBracketSameLine": false,
		"arrowParens": "avoid",
		"printWidth": 80
	}
}
