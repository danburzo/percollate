{
	"name": "percollate",
	"version": "0.2.12",
	"description": "A command-line tool to grab web pages as PDF",
	"main": "index.js",
	"repository": "git@github.com:danburzo/percollate.git",
	"author": "Dan Burzo <danburzo@gmail.com>",
	"license": "MIT",
	"scripts": {
		"test": "mocha ./tests",
		"coverage": "nyc --reporter=text mocha ./tests"
	},
	"dependencies": {
		"commander": "^2.18.0",
		"css": "^2.2.4",
		"got": "^9.2.2",
		"jsdom": "^12.1.0",
		"nunjucks": "^3.1.3",
		"ora": "^3.0.0",
		"puppeteer": "^1.8.0",
		"slugify": "^1.3.1",
		"tmp": "^0.0.33"
	},
	"bin": {
		"percollate": "./cli.js"
	},
	"engines": {
		"node": ">= 8.6.0"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"husky": "^1.0.1",
		"mocha": "^5.2.0",
		"mock-fs": "^4.7.0",
		"nyc": "^13.1.0",
		"prettier": "^1.14.3",
		"pretty-quick": "^1.7.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"nyc": {
		"exclude": [
			"tests",
			"vendor"
		]
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "none",
		"useTabs": true,
		"tabWidth": 4,
		"jsxBracketSameLine": false,
		"arrowParens": "avoid",
		"printWidth": 80
	},
	"keywords": [
		"puppeteer",
		"cli",
		"pdf",
		"readability"
	]
}
